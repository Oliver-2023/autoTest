DOC = """
Sets up and runs the client test for PrintersBulk* and
DevicePrinters* policies.

"""

client_test = 'policy_PrintersBulkAccessMode'
case = ('allowlist',None)

def run(machine):
    host = hosts.create_host(machine)
    job.run_test('policy_DeviceServer', host=host,
                  client_test=client_test, case=case)

parallel_simple(run, machines)
